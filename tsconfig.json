{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "typeRoots": [
      "./types",
      "node_modules/@types"
    ],
    "esModuleInterop": true,
    "types": [
      "@types/node",
      "@types/jest",
      "@nuxtjs/vuetify"
    ],
    // "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules"
  ]
}