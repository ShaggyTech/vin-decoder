{
  "name": "vin-decoder",
  "version": "0.0.1",
  "description": "A web app to decode Vehicle Identification Numbers (VINs) using the NHTSA.dot.gov Vehicles API.",
  "author": "Brandon Eichler <github.com/shaggytech>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "ts-node --project server/tsconfig.json server/server-dev.ts",
    "build": "nuxt-ts build",
    "start": "cross-env NODE_ENV=production ts-node --project 'server/tsconfig.json' server/server-prod.ts",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest -c ./test/config/jest.config.js",
    "test:watch": "jest -c ./test/config/jest.config.js --watch --onlyChanged",
    "test:watchAll": "jest -c ./test/config/jest.config.js --watchAll",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt-ts build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt-ts generate",
    "clean-nuxt": "rimraf dist/* && rimraf .nuxt/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShaggyTech/vin-decoder.git",
    "homepage": "https://github.com/ShaggyTech/vin-decoder"
  },
  "bugs": {
    "url": "https://github.com/ShaggyTech/vin-decoder/issues"
  },
  "homepage": "https://www.shaggytech.com/vin-decoder",
  "keywords": [
    "VIN",
    "WMI",
    "Vehicle Identification Number",
    "NHTSA.dot.gov",
    "NHTSA",
    "National Highway Traffic Safety Administration",
    "decode",
    "decoder",
    "vin decoder",
    "nuxt",
    "typescript"
  ],
  "dependencies": {
    "@nuxt/typescript-runtime": "2.0.0",
    "@nuxtjs/axios": "5.12.2",
    "@nuxtjs/pwa": "3.3.2",
    "@shaggytools/nhtsa-api-wrapper": "1.1.6",
    "@vue/composition-api": "1.0.0-beta.21",
    "cross-env": "7.0.2",
    "express": "4.17.1",
    "nuxt-edge": "npm:nuxt-edge",
    "nuxt-typed-vuex": "0.1.21",
    "vee-validate": "3.4.5"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@nuxt/types": "npm:@nuxt/types-edge",
    "@nuxt/typescript-build": "2.0.3",
    "@nuxt/vue-app": "2.14.11",
    "@nuxtjs/eslint-config-typescript": "3.0.0",
    "@nuxtjs/eslint-module": "2.0.0",
    "@nuxtjs/vuetify": "1.11.2",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/vue": "5.0.4",
    "@types/jest": "26.0.19",
    "@types/node": "14.6.4",
    "@vue/test-utils": "1.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.3.0",
    "chokidar": "3.4.2",
    "consola": "2.15.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "7.2.0",
    "flush-promises": "1.0.2",
    "jest": "26.4.2",
    "jest-serializer-vue": "2.0.2",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.3.0",
    "vue-jest": "3.0.7"
  }
}
