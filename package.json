{
  "name": "vin-decoder",
  "version": "0.0.1",
  "description": "A web app to decode Vehicle Identification Numbers (VIN) using the NHTSA.dot.gov Vehicles API.",
  "author": "Brandon Eichler <github.com/shaggytech>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "ts-node --project server/tsconfig.json server/server-dev.ts",
    "build": "yarn clean:nuxt && nuxt-ts build",
    "start": "cross-env NODE_ENV=production ts-node --project 'server/tsconfig.json' server/server-prod.ts",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest -c ./test/config/jest.config.js",
    "test:watch": "jest -c ./test/config/jest.config.js --watch --onlyChanged",
    "test:watchAll": "jest -c ./test/config/jest.config.js --watchAll",
    "build:gh-pages": "yarn clean:nuxt && cross-env DEPLOY_ENV=GH_PAGES nuxt-ts build",
    "generate:gh-pages": "yarn clean:nuxt && cross-env DEPLOY_ENV=GH_PAGES nuxt-ts generate",
    "clean:nuxt": "rimraf dist/* && rimraf .nuxt/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShaggyTech/vin-decoder.git",
    "homepage": "https://github.com/ShaggyTech/vin-decoder"
  },
  "bugs": {
    "url": "https://github.com/ShaggyTech/vin-decoder/issues"
  },
  "homepage": "https://www.shaggytech.com/vin-decoder",
  "keywords": [
    "VIN",
    "WMI",
    "Vehicle Identification Number",
    "NHTSA.dot.gov",
    "NHTSA",
    "National Highway Traffic Safety Administration",
    "decode",
    "decoder",
    "vin decoder",
    "nuxt",
    "typescript"
  ],
  "dependencies": {
    "@nuxt/typescript-runtime": "2.0.1",
    "@nuxtjs/axios": "5.12.5",
    "@nuxtjs/pwa": "3.3.4",
    "@nuxtjs/composition-api": "0.18.1",
    "@shaggytools/nhtsa-api-wrapper": "1.1.11",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "nuxt-edge": "npm:nuxt-edge@2.15.0-26856070.8350560",
    "nuxt-typed-vuex": "0.1.22",
    "nuxt-webfontloader": "1.1.0",
    "vee-validate": "3.4.5"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@nuxt/types": "npm:@nuxt/types-edge@2.15.0-26856070.8350560",
    "@nuxt/typescript-build": "2.0.3",
    "@nuxt/vue-app": "2.14.12",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/vuetify": "1.11.2",
    "@mdi/js": "5.9.55",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/vue": "5.6.1",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.22",
    "@vue/test-utils": "1.1.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "26.6.3",
    "chokidar": "3.5.1",
    "consola": "2.15.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-vue": "7.5.0",
    "flush-promises": "1.0.2",
    "jest": "26.6.3",
    "jest-localstorage-mock": "2.4.6",
    "jest-serializer-vue": "2.0.2",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "26.4.4",
    "vue-jest": "3.0.7"
  }
}
