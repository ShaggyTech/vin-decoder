{
  "name": "vin-decoder",
  "version": "0.0.1",
  "description": "A web app to decode Vehicle Identification Numbers (VINs) using the NHTSA.dot.gov Vehicles API.",
  "author": "Brandon Eichler <github.com/shaggytech>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "ts-node --project server/tsconfig.json server/server-dev.ts",
    "build": "nuxt-ts build",
    "start": "cross-env NODE_ENV=production ts-node --project 'server/tsconfig.json' server/server-prod.ts",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest -c ./test/config/jest.config.js",
    "test:watch": "jest -c ./test/config/jest.config.js --watch --onlyChanged",
    "test:watchAll": "jest -c ./test/config/jest.config.js --watchAll",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt-ts build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt-ts generate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShaggyTech/vin-decoder.git",
    "homepage": "https://github.com/ShaggyTech/vin-decoder"
  },
  "bugs": {
    "url": "https://github.com/ShaggyTech/vin-decoder/issues"
  },
  "homepage": "https://www.shaggytech.com/vin-decoder",
  "keywords": [
    "VIN",
    "WMI",
    "Vehicle Identification Number",
    "NHTSA.dot.gov",
    "NHTSA",
    "National Highway Traffic Safety Administration",
    "decode",
    "decoder",
    "vin decoder",
    "nuxt",
    "typescript"
  ],
  "dependencies": {
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/axios": "latest",
    "@nuxtjs/dotenv": "latest",
    "@nuxtjs/pwa": "latest",
    "@shaggytools/nhtsa-api-wrapper": "latest",
    "@vue/composition-api": "latest",
    "cross-env": "latest",
    "express": "latest",
    "nuxt-edge": "latest",
    "nuxt-typed-vuex": "latest",
    "vee-validate": "latest"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "latest",
    "@nuxt/vue-app": "latest",
    "@nuxtjs/eslint-config": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@nuxtjs/eslint-module": "latest",
    "@nuxtjs/vuetify": "latest",
    "@types/jest": "latest",
    "@types/node": "latest",
    "@vue/test-utils": "latest",
    "babel-eslint": "latest",
    "babel-jest": "latest",
    "chokidar": "latest",
    "consola": "2.12.1",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-nuxt": "latest",
    "eslint-plugin-prettier": "latest",
    "flush-promises": "latest",
    "jest": "latest",
    "jest-serializer-vue": "latest",
    "prettier": "latest",
    "ts-jest": "latest",
    "vue-jest": "latest"
  }
}
