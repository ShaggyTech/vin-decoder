{
  "name": "vin-decoder",
  "version": "0.0.1",
  "description": "A web app to decode Vehicle Identification Numbers (VINs) using the NHTSA.dot.gov Vehicles API.",
  "author": "Brandon Eichler <github.com/shaggytech>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.ts",
    "build": "nuxt-ts build",
    "start": "cross-env NODE_ENV=production ts-node --project 'server/tsconfig.json' server/index.ts",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest -c ./test/config/jest.config.js",
    "test-watch": "jest -c ./test/config/jest.config.js --watch --onlyChanged",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt-ts build",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt-ts generate"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShaggyTech/vin-decoder.git",
    "homepage": "https://github.com/ShaggyTech/vin-decoder"
  },
  "bugs": {
    "url": "https://github.com/ShaggyTech/vin-decoder/issues"
  },
  "homepage": "https://www.shaggytech.com/vin-decoder",
  "keywords": [
    "VIN",
    "WMI",
    "Vehicle Identification Number",
    "NHTSA.dot.gov",
    "NHTSA",
    "National Highway Traffic Safety Administration",
    "decode",
    "decoder",
    "vin decoder",
    "nuxt",
    "typescript"
  ],
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.5",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@shaggytools/nhtsa-api-wrapper": "^1.1.3",
    "@vue/composition-api": "^0.4.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.0.0",
    "vee-validate": "^3.2.5",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxt/vue-app": "^2.11.0",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.2",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^1.18.9",
    "nuxt-typed-vuex": "^0.1.17",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "vue-jest": "^4.0.0-0"
  }
}
