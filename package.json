{
  "name": "vin-decoder",
  "version": "0.0.1",
  "description": "A web app to decode Vehicle Identification Numbers (VIN) using the NHTSA.dot.gov Vehicles API.",
  "author": "Brandon Eichler <github.com/shaggytech>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "nuxt",
    "build": "yarn clean:nuxt && nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "test": "jest -c ./test/config/jest.config.js",
    "test:watch": "jest -c ./test/config/jest.config.js --watch --onlyChanged",
    "test:watchAll": "jest -c ./test/config/jest.config.js --watchAll",
    "build:gh-pages": "yarn clean:nuxt && cross-env DEPLOY_ENV=GH_PAGES nuxt build",
    "generate:gh-pages": "yarn clean:nuxt && cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "clean:nuxt": "rimraf dist/* && rimraf .nuxt/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ShaggyTech/vin-decoder.git",
    "homepage": "https://github.com/ShaggyTech/vin-decoder"
  },
  "bugs": {
    "url": "https://github.com/ShaggyTech/vin-decoder/issues"
  },
  "homepage": "https://www.shaggytech.com/vin-decoder",
  "keywords": [
    "VIN",
    "WMI",
    "Vehicle Identification Number",
    "NHTSA.dot.gov",
    "NHTSA",
    "National Highway Traffic Safety Administration",
    "decode",
    "decoder",
    "vin decoder",
    "nuxt",
    "typescript"
  ],
  "dependencies": {
    "@nuxtjs/axios": "5.13.6",
    "@nuxtjs/composition-api": "0.24.4",
    "@shaggytools/nhtsa-api-wrapper": "2.0.2",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "nuxt-edge": "2.16.0-27069135.35c6ac41",
    "nuxt-typed-vuex": "0.2.0",
    "nuxt-webfontloader": "1.1.0",
    "vee-validate": "3.4.10"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/preset-env": "7.14.5",
    "@mdi/js": "5.9.55",
    "@nuxt/types": "npm:@nuxt/types-edge@2.16.0-27069135.35c6ac41",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxt/vue-app": "2.15.7",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/vuetify": "1.12.1",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/vue": "5.6.2",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.3",
    "@vue/test-utils": "1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.0.2",
    "chokidar": "3.5.2",
    "consola": "2.15.3",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-vue": "7.11.1",
    "flush-promises": "1.0.2",
    "jest": "27.0.4",
    "jest-localstorage-mock": "2.4.14",
    "jest-serializer-vue": "2.0.2",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.3",
    "vue-jest": "3.0.7"
  }
}
